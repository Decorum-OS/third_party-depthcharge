#
# Generic Options
#
CONFIG_GPL=y
CONFIG_REMOTEGDB=y
CONFIG_CHROMEOS=y

#
# Architecture Options
#
CONFIG_ARCH_X86=y

#
# Console Options
#
CONFIG_SKIP_CONSOLE_INIT=y
# CONFIG_VGA_VIDEO_CONSOLE is not set
CONFIG_COREBOOT_VIDEO_CONSOLE=y

#
# Drivers
#
# CONFIG_SPEAKER is not set
# CONFIG_STORAGE is not set
CONFIG_USB=y
CONFIG_USB_UHCI=y
CONFIG_USB_OHCI=y
CONFIG_USB_EHCI=y
# CONFIG_USB_XHCI is not set
