#
# Generic Options
#
CONFIG_GPL=y
CONFIG_REMOTEGDB=y
CONFIG_CHROMEOS=y

#
# Architecture Options
#
CONFIG_ARCH_ARM_V7=y

#
# Console Options
#
CONFIG_SKIP_CONSOLE_INIT=y
CONFIG_S5P_SERIAL_CONSOLE=y
CONFIG_COREBOOT_VIDEO_CONSOLE=y

#
# Drivers
#
# CONFIG_STORAGE is not set
CONFIG_TIMER_MCT=y
CONFIG_USB=y
# CONFIG_USB_OHCI is not set
# CONFIG_USB_EHCI is not set
CONFIG_USB_XHCI=y
